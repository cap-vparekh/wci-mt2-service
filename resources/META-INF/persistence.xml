<?xml version="1.0" encoding="UTF-8"?>

<persistence version="2.0"
  xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <!-- use transaction-type RESOURCE_LOCAL because we do not have a container -->
  <persistence-unit name="refsetservice-ds"
    transaction-type="RESOURCE_LOCAL">

    <!-- JPA persistence provider. -->
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>

    <!-- import terminology models from common -->
    <class>org.ihtsdo.refsetservice.model.Concept</class>
    <class>org.ihtsdo.refsetservice.model.Refset</class>
    <class>org.ihtsdo.refsetservice.model.RefsetEditHistory</class>
    <class>org.ihtsdo.refsetservice.model.Edition</class>
    <class>org.ihtsdo.refsetservice.model.Organization</class>
    <class>org.ihtsdo.refsetservice.model.Project</class>
    <class>org.ihtsdo.refsetservice.model.DefinitionClause</class>
    <class>org.ihtsdo.refsetservice.model.DefinitionClauseEditHistory</class>
    <class>org.ihtsdo.refsetservice.model.WorkflowHistory</class>
    <class>org.ihtsdo.refsetservice.model.UpgradeInactiveConcept</class>
    <class>org.ihtsdo.refsetservice.model.UpgradeReplacementConcept</class>
    <class>org.ihtsdo.refsetservice.model.Team</class>
    <class>org.ihtsdo.refsetservice.model.DiscussionPost</class>
    <class>org.ihtsdo.refsetservice.model.DiscussionThread</class>
    <class>org.ihtsdo.refsetservice.model.AuditEntry</class>
    <class>org.ihtsdo.refsetservice.model.Artifact</class>
    <class>org.ihtsdo.refsetservice.model.InviteRequest</class>
   
        
    <!--  import models for auth -->
    <class>org.ihtsdo.refsetservice.model.User</class>
    
    <!-- import mapping models -->
    <class>org.ihtsdo.refsetservice.model.MapUser</class>
    <class>org.ihtsdo.refsetservice.model.AdditionalMapEntryInfo</class>
    <class>org.ihtsdo.refsetservice.model.MapEntry</class>
    <class>org.ihtsdo.refsetservice.model.MapPrinciple</class>
    <class>org.ihtsdo.refsetservice.model.MapRelation</class>
    <class>org.ihtsdo.refsetservice.model.MapAdvice</class>
    <class>org.ihtsdo.refsetservice.model.MapNote</class>
    <class>org.ihtsdo.refsetservice.model.Mapping</class>
    <class>org.ihtsdo.refsetservice.model.MapSet</class>
    <class>org.ihtsdo.refsetservice.model.MapAgeRange</class>
    <class>org.ihtsdo.refsetservice.model.MapProject</class>
    <class>org.ihtsdo.refsetservice.model.MapReportDefinition</class>
    
  </persistence-unit>
</persistence>
